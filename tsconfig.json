{
  "compilerOptions": {
    "target": "esnext",
    "declaration": true,
    "module": "ES2022",
    "jsx": "preserve",
    "moduleResolution": "node",
    "lib": [
      "esnext",
      "esnext.asynciterable",
      "dom"
    ],
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "allowImportingTsExtensions": true,
    "plugins": [{
      "transform": "ts-transformer-keys/transformer"
    }]
  }
}